<!-- FG Classic should use fgf and FU Unity will load ttf -->
<root>
	<!-- Default fonts - REQUIRED -->
	<font name="defaultstringcontrol">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>
	<font name="defaultnumbercontrol">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>


	<!-- Chat fonts - REQUIRED -->
	<font name="chatfont">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#261A12" />
	</font>
	<font name="emotefont">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#880000" />
	</font>
	<font name="msgfont">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#660066" />
	</font>
	<font name="oocfont">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#005500" />
	</font>

	<!-- Chat fonts -->
	<font name="chatnpcfont">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000066" />
	</font>


	<!-- Standard text fields -->

	<font name="sheettext">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>

	<!-- Tables -->
	<font name="tablerange">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>
	<font name="tableheader">
		<ttf file="graphics/fonts/SW-Black.ttf" name="SW Black" size="16" />
		<color value="#000000" />
	</font>
	<font name="tabledata">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>


	<!-- Formatted text fields -->
	<font name="reference-h">
		<ttf file="graphics/fonts/SW-Black.ttf" name="SW Black" size="24" />
		<color value="660000" />
	</font>

	<font name="reference-r">
		<ttf file="graphics/fonts/SW-Regular.ttf" name="SW Regular" size="16" />
		<color value="#000000" />
	</font>

<font name="reference-i">
	<ttf file="graphics/fonts/SW-Italic.ttf" name="SW Italic" size="16" />
	<color value="#000000" />
</font>

<font name="reference-b">
	<ttf file="graphics/fonts/SW-Black.ttf" name="SW Black" size="16" />
	<color value="#000000" />
</font>
<font name="reference-bi">
	<ttf file="graphics/fonts/SW-BlackItalic.ttf" name="SW Black Italic" size="16" />
	<color value="#000000" />
</font>
<font name="reference-b-large">
	<ttf file="graphics/fonts/SW-Black.ttf" name="SW Black" size="18" />
	<color value="#000000" />
</font>



</root>
